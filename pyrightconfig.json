{
  "ignore": ["**/node_modules", "**/__pycache__"],
  "pythonPlatform": "Linux",
  "pythonVersion": "3.12",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "stubPath": "typings",
  "typeCheckingMode": "basic",
  "venvPath": ".",
  "venv": ".venv",
  "strictParameterNoneValue": false,
  "executionEnvironments": [{ "root": "." }]
}
